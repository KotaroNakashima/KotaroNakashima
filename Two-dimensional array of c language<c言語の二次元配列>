#include<stdio.h>

int main(void)
{
    int scores[2][3];
    scores[0][0]=80;
    scores[0][1]=77;
    scores[0][2]=65;
    scores[1][0]=64;
    scores[1][1]=78;
    scores[1][2]=98;

    for(int i=0; i<2; i++){
        printf("%d人目の点数を表示します\n",i+1);
    for(int j=0; j<3; j++){
        printf("%d科目め:%d\n",j+1,scores[i][j]);
    }
    }
    return 0;
}
