<body>
    <script>
        "use strict";
        function x(y){
            if(y%3===0 && y%5===0){
                return "AB";
            }else if(y%3===0){
                return "A";
            }else if(y%5===0){
                return "B";
            }else{
                return "C";
            }
        }
        console.log(x(7));
    </script> 
</body>
</html>
