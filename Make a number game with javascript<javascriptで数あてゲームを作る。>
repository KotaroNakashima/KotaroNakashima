    <script>
        "use strict";
        const x=Math.floor(Math.random()*6);
        const y=parseInt(prompt("数あてゲーム。0~5の数を入力してね。"));
        if(x===y){
        alert("あたり");
        }else{
        alert("外れ");
        }
    </script>
