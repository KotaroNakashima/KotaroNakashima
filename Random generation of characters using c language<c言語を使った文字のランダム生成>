#include<stdio.h>
#include<stdlib.h>
#include<time.h>

int main(void)
{
    printf("あなたの運勢を占います\n");
    srand((unsigned)time(NULL));
    int x=rand()%3+1;
    
    if(x==1){
          printf("大吉\n");
    }else if(x==2){
          printf("中吉\n");
    }else if`(x==3){
          printf("小吉\n");
     }
     return 0;
}
