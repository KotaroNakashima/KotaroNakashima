//CSS
* {
    margin: 0;
    padding: 0;
  }
  
  ul,
  li {
    list-style: none;
  }
  
  #carousel-wrapper {
    display: flex;
    align-items: center;
    justify-content: center;
  }
  
  #carousel-area {
    width: 970px;
    overflow: hidden;
    margin: 0 5px;
  }
  
  #carousel-area ul {
    display: flex;
    margin: 0 5px;
  }
  
  #carousel-area li {
    margin: 10px 5px;
  }
  
  button {
    width: 60px;
    height: 25px;
    border-radius: 5px;
  }
  
  //Javascript
  $(function() {
    $("#carousel-area li:last").prependTo("#carousel-area ul");
    $("#carousel-area ul").css({ marginLeft: "-155px" });
    $(".btn").click(function() {
      var btnId = $(this).attr("id");
      if(btnId == "prevBtn"){
        $("#carousel-area ul").animate({
          marginLeft: "-315px"
          },function(){
          $("#carousel-area li:first").appendTo("#carousel-area ul");
          $("#carousel-area ul").css({ marginLeft: "-155px" });
        })
      }else if(btnId == "nextBtn"){
        $("#carousel-area ul").animate({
          marginLeft: "5px"
          },function(){
          $("#carousel-area li:last").prependTo("#carousel-area ul");
          $("#carousel-area ul").css({ marginLeft: "-155px" });
        })
      }
    })
  })
