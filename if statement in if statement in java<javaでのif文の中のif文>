public class nihao{
    public static void main(String[] args){
    int a=100;
    System.out.println("あなたの点数は"+a+"点です。");
    if(a<70){
        System.out.println("平均まであと"+a+"点です。");
        System.out.println("頑張りましょう");
    }else{
        if(a==100){
        System.out.println("おめでとうございます。");
        }else{
        System.out.println("あともう少しです。");
        }
    }
    }
}
